		try {
			input = socket.getInputStream();
			out = new OutputStreamWriter(socket.getOutputStream());
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

	public void start() {
		try {
		} catch (Exception e) {
			
		}
	}


}
